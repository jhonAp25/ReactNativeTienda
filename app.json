{
  "expo": {
    "name": "react Tienda Ap",
    "slug": "react-expo",
    "privacy": "public",
    "description": "Aplicativo tienda virtual(solo admin)",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LogoApOf.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/LogoApOf.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.jhonap25.reactexpo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
    
  }
}
